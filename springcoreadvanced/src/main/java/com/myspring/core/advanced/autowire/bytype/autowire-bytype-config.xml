<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p" xmlns:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd"
	default-lazy-init="true">

	<bean name="address1" class="com.myspring.core.advanced.autowire.byname.Address"
		c:houseNbr="1" c:streetName="Creek Drive1" />

	<bean name="address2" class="com.myspring.core.advanced.autowire.bytype.address.Address"
		c:houseNbr="2" c:streetName="Creek Drive2" />

	<bean name="accountHolder"
		class="com.myspring.core.advanced.autowire.byname.AccountHodler"
		autowire="" />

</beans>